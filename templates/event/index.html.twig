{% extends 'base.html.twig' %}

{% block title %}Hello EventController!
{% endblock %}

{% block body %}
	{% for event in events %}
		<div class="w-[500px]">
			{% for file in event.fileId %}
				<img class="" src="{{asset('assets/images/' ~ file.url)}}" alt="Event Image" />
			{% endfor %}

			<h1 class="text-3xl">{{ event.title }}</h1>
			<p>{{ event.content }}</p>
		</div>
	{% endfor %}
{% endblock %}
